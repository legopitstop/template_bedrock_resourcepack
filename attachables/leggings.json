{
  "format_version": "1.10.0",
  "minecraft:attachable": {
    "description": {
      "identifier": "MOD_ID:leggings.player",
      "item": {
        "MOD_ID:leggings": "q.is_owner_identifier_any('minecraft:player')"
      },
      "materials": {
        "default": "armor",
        "enchanted": "armor_enchanted"
      },
      "textures": {
        "default": "textures/models/armor/leggings_2",
        "enchanted": "textures/misc/enchanted_item_glint"
      },
      "geometry": {
        "default": "geometry.player.armor.leggings"
      },
      "scripts": {
        "parent_setup": "variable.leg_layer_visible = 0.0;",
        "animate": ["offset"]
      },
      "animations": {
        "offset": "animation.armor.leggings.offset"
      },
      "render_controllers": ["controller.render.armor"]
    }
  }
}
