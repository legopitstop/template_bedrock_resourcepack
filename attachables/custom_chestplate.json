{
  "format_version": "1.10.0",
  "minecraft:attachable": {
    "description": {
      "identifier": "mod_id:custom_chestplate.player",
      "item": {
        "mod_id:custom_chestplate": "q.is_owner_identifier_any('minecraft:player')"
      },
      "materials": {
        "default": "armor",
        "enchanted": "armor_enchanted"
      },
      "textures": {
        "default": "textures/models/armor/custom_1",
        "enchanted": "textures/misc/enchanted_item_glint"
      },
      "geometry": {
        "default": "geometry.player.armor.chestplate"
      },
      "scripts": {
        "parent_setup": "variable.chest_layer_visible = 0.0;",
        "animate": ["offset"]
      },
      "animations": {
        "offset": "animation.armor.chestplate.offset"
      },
      "render_controllers": ["controller.render.armor"]
    }
  }
}
